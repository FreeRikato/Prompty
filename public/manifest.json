{
    "manifest_version": 3,
    "name": "Prompty",
    "version": "1.0",
    "permissions": ["activeTab", "scripting", "storage"],
    "action": {
        "default_popup": "index.html"
    },
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}